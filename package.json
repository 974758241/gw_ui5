{
  "name": "ui5-list",
  "displayName": "ui5-list",
  "description": "目前主要下载SAPUI5程序 从ABAP repository下载",
  "version": "0.0.4",
  "keywords": [
    "ui5 list",
    "gateway",
    "ui5 download"
  ],
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:UI5.getUI5List"
  ],
  "main": "./out/extension.js",
  "icon": "images/sap-ui5.png",
  "contributes": {
    "commands": [
      {
        "command": "UI5.getUI5List",
        "title": "从UI5服务器导入UI5工程"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "UI5.getUI5List",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "install:all": "npm install && npm --prefix ./webview-ui install ./webview-ui",
    "start:webview": "npm --prefix ./webview-ui run start",
    "build:webview": "npm --prefix ./webview-ui run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "fast-xml-parser": "^4.0.8",
    "glob": "^7.1.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "publisher": "zhuguoyou",
  "dependencies": {
    "axios": "^0.27.2",
    "fast-xml-parser": "^4.0.8"
  },
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/974758241/gw_ui5/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/974758241/gw_ui5"
  },
  "homepage": "https://github.com/974758241/gw_ui5/README.md"
}
